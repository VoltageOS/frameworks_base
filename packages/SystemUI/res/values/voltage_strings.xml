<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Screenshot delete action -->
    <!-- Label for UI element which allows deleting the screenshot [CHAR LIMIT=30] -->
    <string name="screenshot_delete_label">Delete</string>
    <!-- Content description indicating that tapping the element will allow deleting the screenshot [CHAR LIMIT=NONE] -->
    <string name="screenshot_delete_description">Delete screenshot</string>

    <!-- Name of the nfc status bar icon. -->
    <string name="status_bar_nfc">NFC</string>

    <!-- Accessibility label for nfc icon (not shown on screen) [CHAR LIMIT=NONE] -->
    <string name="accessibility_status_bar_nfc">Nfc</string>

    <!-- Screen record delete action -->
    <!-- Label for notification action to delete a screen recording file [CHAR LIMIT=35] -->
    <string name="screenrecord_delete_label">Delete</string>
    <!-- A toast message shown after successfully deleting a screen recording [CHAR LIMIT=NONE] -->
    <string name="screenrecord_delete_description">Screen recording deleted</string>

    <!-- Screenshot Delete Toast -->
    <string name="delete_screenshot_toast">Screenshot deleted</string>

    <!-- Screen record -->
    <string name="screenrecord_stopdot_label">Show stop dot</string>
    <string name="screenrecord_stopdot_summary">Longpress to move it</string>
    <string name="screenrecord_lowquality_label">Lower quality</string>
    <string name="screenrecord_lowquality_summary">For smaller file size</string>
</resources>
