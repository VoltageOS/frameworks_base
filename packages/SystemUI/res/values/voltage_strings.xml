    <string name="status_bar_items_title">Status bar icons</string>
    <string name="status_bar_call_strength">Call strength</string>
    <string name="status_bar_data_saver">Data saver</string>
    <string name="status_bar_vpn">VPN</string>
    <string name="status_bar_roaming">Roaming indicator</string>
    <string name="status_bar_ims">VoLTE/VoWiFi</string>

